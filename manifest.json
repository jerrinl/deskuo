{
    "manifest_version": 2,

    "name": "Asana UO Desk Info",
    "description": "UO tool to quickly grab user email in easy terminal format.",
    "version": "2.3",

    "permissions": [
        "<all_urls>",
        "clipboardWrite",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}